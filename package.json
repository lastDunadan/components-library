{
  "name": "next.js-template",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./pages ./src --ext .ts --ext .tsx --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "types-check": "tsc --noEmit",
    "audit": "npm audit --audit-level high --omit=dev",
    "prepare": "husky install",
    "prepush": "npm run lint && npm run types-check && npm run audit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./docs/storybook",
    "generate-typescript-docs": "typedoc",
    "pregenerate-types-from-yaml": "npm run generate-swagger-docs",
    "generate-types-from-yaml": "spec2ts openapi docs/swagger.json -o src/types",
    "generate-swagger-docs": "next-swagger-doc-cli swagger-doc.json --output docs/swagger.json",
    "generate-docs": "npm run generate-typescript-docs && npm run build-storybook && npm run generate-swagger-docs && cp swaggerTemplate.html ./docs/swagger.html"
  },
  "dependencies": {
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.11",
    "styled-icons": "10.47.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@spec2ts/cli": "^2.0.0-beta.13",
    "@spec2ts/jsonschema": "^2.0.0-beta.8",
    "@storybook/addon-a11y": "^7.1.1",
    "@storybook/addon-actions": "^7.1.1",
    "@storybook/addon-controls": "^7.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/nextjs": "^7.1.1",
    "@storybook/preset-create-react-app": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.1",
    "next-swagger-doc": "^0.3.6",
    "prettier": "^2.7.1",
    "storybook": "^7.1.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typedoc": "^0.23.15",
    "typescript": "4.8.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "webpack": "^5.74.0"
  }
}
